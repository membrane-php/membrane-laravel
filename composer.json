{
  "name": "membrane/laravel",
  "type": "library",
  "license": "MPL-2.0",
  "autoload": {
    "psr-4": {
      "Membrane\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Membrane\\Laravel\\Tests\\": "tests/",
      "Membrane\\Laravel\\Fixtures\\": "tests/fixtures/"
    }
  },
  "require": {
    "php": "^8.2.0",
    "crell/api-problem": "^3.6.1",
    "illuminate/console": "^9.0 || ^10.0 || ^11.0",
    "illuminate/http": "^9.0 || ^10.0 || ^11.0",
    "illuminate/support": "^9.0 || ^10.0 || ^11.0",
    "nyholm/psr7": "^1.8",
    "membrane/membrane": "^0.8",
    "membrane/openapi-router": "^0.5",
    "symfony/psr-http-message-bridge": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5.40",
    "phpstan/phpstan": "^2.1.1",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Membrane\\Laravel\\ServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
